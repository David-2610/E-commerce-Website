{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "routes/**",
            "Config/**",
            "Models/**",
            "middleware/**",
            ".env"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/server.js"
      }
    ]
  }
  